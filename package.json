{
  "name": "nextjs-srt-deepl",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "eslint --fix . --ext .ts,.js,.tsx,.jsx",
    "format": "prettier --write .",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "next": "15.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@t3-oss/env-nextjs": "0.13.4",
    "@tailwindcss/postcss": "4.1.7",
    "@types/node": "22.15.21",
    "@types/react": "19.1.5",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "babel-loader": "10.0.0",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react": "7.37.5",
    "husky": "9.1.7",
    "lint-staged": "16.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "4.1.7",
    "typescript": "5.8.3",
    "zod": "3.25.17"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "centra": "2.6.0"
    },
    "overrides": {
      "node-fetch": "latest",
      "semver": "latest"
    }
  },
  "engines": {
    "node": ">=22.x"
  }
}
